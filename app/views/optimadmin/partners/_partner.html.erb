<div class="row collapse module-list-index <%= cycle('odd', 'even') %>" data-equalizer="module-list" id="item_<%= partner_presenter.id %>">

    <div class="large-1 hide-for-medium-down columns" data-equalizer-watch="module-list">
      <span class="drag-helper"></span>
    </div>



    <div class="large-4 small-8 columns" data-equalizer-watch="module-list">

    <%= partner_presenter.toggle_title %>
  </div>


    <div class="large-2 hide-for-medium-down columns" data-equalizer-watch="module-list">
      <%= partner_presenter.edit_image(:image)%>
    </div>

  <div class="large-2 small-4 columns" data-equalizer-watch="module-list">
    <%= partner_presenter.toggle_link(:display_on_partners_page) %>
  </div>



    <div class="large-1 small-4 columns" data-equalizer-watch="module-list">
      <%= partner_presenter.toggle_link(:display_in_footer) %>
    </div>

  <div class="large-1 small-4 columns" data-equalizer-watch="module-list">
    <%= partner_presenter.edit_link %>
  </div>

  <div class="large-1 hide-for-medium-down columns last" data-equalizer-watch="module-list">
    <%= partner_presenter.delete_link %>
  </div>

  <div class="row module-list-detail hide" id="index-list-<%= partner_presenter.id %>">
    <div class="small-12 columns module-list-summary last">
      <%= partner_presenter.optimadmin_summary %>
    </div>
  </div>
</div>
